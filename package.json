{
  "name": "eslint-teamcity",
  "version": "2.1.0",
  "description": "An ESLint formatter plugin for TeamCity",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write ./**/*.js",
    "test": "mocha \"./test/**/*.spec.js\"",
    "test:travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/andreogle/eslint-teamcity.git"
  },
  "keywords": [
    "eslint",
    "teamcity",
    "formatter",
    "linting"
  ],
  "author": "Andre Ogle <ogleandre@gmail.com> (https://github.com/andreogle)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreogle/eslint-teamcity/issues"
  },
  "dependencies": {
    "fs-extra": "^7.0.x"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.x",
    "eslint-config-airbnb-base": "^13.1.x",
    "eslint-plugin-import": "^2.17.x",
    "husky": "^1.3.x",
    "lint-staged": "^8.1.7",
    "mocha": "^5.2.x",
    "nyc": "^14.1.x",
    "prettier": "^1.17.x",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.x"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
