{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "function-paren-newline": 0,
    "global-require": 0,
    "no-console": [1, { "allow": ["info", "warn", "error"] }],
    "object-curly-newline": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0,

    "import/no-dynamic-require": 0
  }
}
